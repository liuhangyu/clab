ob1=OBI(cluster=1244);
ob2=OBI(cluster=1244);

ob2.rs0.set master=ob1.rs0;

ob1.reboot;
ob2.reboot;

sleep 60;
ob1.source include/merge.inc;
sleep 10;

ob1.prepare;
sleep 60;

ob1.start_check;

ob1.major_freeze;
ob1.check_cs_version 2;

ob1.stop_check;

ob1.force_set_master_cluster ob2;

ob1.start_check; 
sleep 60;
ob1.stop_check;

ob1.force_set_master_cluster ob1;

ob1.start_check; 
sleep 60;
ob1.stop_check;

ob1.switch_cluster ob2;

ob1.start_check; 
sleep 60;
ob1.stop_check;
